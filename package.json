{
  "name": "car_store",
  "version": "1.0.0",
  "description": "Simple Car Store REST API powered by json-server",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "start:raw": "json-server --watch db.json --port 3001 --routes routes.json",
    "reset": "node tools/reset.js"
  },
  "dependencies": {
    "json-server": "^0.17.4"
  }
}